#!/bin/bash
echo "------------------------------------------------------------------------"
echo `cd "${0%/*}" 2>/dev/null; echo $PWD/${0##*/}`
echo "------------------------------------------------------------------------"
set -x
source "${HOME3P}/utilx"
[ $? -ne 0 ] && exit 1
#
download .      || exit 1
download flac   || exit 1
download mad    || exit 1
download id3tag || exit 1
#
exit 0
